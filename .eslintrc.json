{
    "root": true,
    "env": {
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base",
        "plugin:jest/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
        "jest"
    ],
    "rules": {
        "no-console": 0
    },
    "settings": {
        "import/resolver": {
            "webpack": {
                "config": "webpack.prod.config.js"
            }
        }
    }
}